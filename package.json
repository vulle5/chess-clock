    "@types/react-native": "~0.69.1",
    "expo": "~46.0.16",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-native": "0.69.6",
    "timer-node": "^5.0.6",
    "zustand": "^4.1.3",
    "expo-screen-orientation": "~4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "babel-plugin-module-resolver": "^4.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
